      {
        "viewName": "ActivityWithBikeData",
        "createSql": "CREATE VIEW `${VIEW_NAME}` AS SELECT b.name as bike_name, b.uid as bike_uid, a.title as activity_title, a.description as activity_description, a.is_completed as activity_is_completed, a.ride_uid as activity_ride_uid, a.created_date as activity_created_date, a.due_date as activity_due_date, a.done_date as activity_done_date, a.is_ebike_specific as activity_is_ebike_specific, a.uid as activity_uid FROM Activity a LEFT JOIN Bike b ON b.uid = a.bike_uid ORDER BY a.due_date DESC;"
      },
